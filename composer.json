{
	"name": "valiant/yii2-upload-behavior",
	"type": "yii2-extension",
	"description": "Yii2 file/image upload behavior for ActiveRecord",
	"keywords": [
		"file",
		"image",
		"upload",
		"behavior",
		"resize",
		"thumbnail"
	],
	"license": "MIT",
	"homepage": "https://github.com/one-more-developer/yii2-upload-behavior",
	"authors": [
		{
			"name": "Igor Prokofev",
			"email": "mr.igor.prokofev@gmail.com",
			"homepage": "https://github.com/one-more-developer",
			"role": "Developer"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"masterexploder/phpthumb": "*"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/yii-dream-team/PHPThumb"
		}
	],
	"autoload": {
		"psr-4": {
			"valiant\\behaviors\\": "./src"
		}
	}
}